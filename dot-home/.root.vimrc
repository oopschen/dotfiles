" basic conf
set number
syntax on
set expandtab
set smarttab
set shiftwidth=2
set softtabstop=2
set tabstop=2
set autoindent
set smartindent

set history=128
set autoread
set cmdheight=1
set hlsearch
set magic
set incsearch
set laststatus=2
set fileencodings=utf-8,gbk,gb2312

set t_Co=256
set timeoutlen=700
set pastetoggle=<F12>
set ignorecase
set smartcase
filetype indent plugin on
set hidden
set bkc=yes
set wrap
set linebreak
" end
