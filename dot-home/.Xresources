!Xft.dpi: 96
Xft.antialias: true
Xft.hinting: full
Xft.hintstyle: hintslight
Xft.rgba: rgb

! urxvt
URxvt.scrollBar: false
URxvt.scrollstyle: plain
URxvt.font: xft:Source Code Pro:size=13, xft:WenQuanYi Zen Hei Mono:size=13
URxvt.boldFont: xft:Source Code Pro:size=13, xft:WenQuanYi Zen Hei Mono:size=13
URxvt.termName: rxvt-unicode
URxvt.saveLines: 20000
URxvt.foreground: #FFFFFF
URxvt.background: #000000
URxvt.cursorColor: #4D90F8
URxvt.preeditType: Root

! urxvt shortcut
URxvt.perl-ext-common: default,keyboard-select,matcher,selection-to-clipboard
URxvt.keysym.C-S-K: perl:keyboard-select:activate
URxvt.keysym.C-S-S: perl:keyboard-select:search

! matcher
URxvt.url-launcher: google-chrome-stable
URxvt.keysym.C-S-U: perl:matcher:select

